<!DOCTYPE html>
<html lang="vi">
<head>
	<!-- META -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="keywords" content="" />
    <meta name="author" content="" />
    <meta name="robots" content="" />    
    <meta name="description" content="" />
    
    <!-- FAVICONS ICON -->
    <link rel="icon" href="/hoanmy/images/favicon.ico" type="image/x-icon" />
    <link rel="shortcut icon" type="image/x-icon" href="/hoanmy/images/favicon.png" />
    
    <!-- PAGE TITLE HERE -->
    <title>Trang Chủ | Công Ty TNHH ĐT DV XD Hoàn Mỹ</title>
    
    <!-- MOBILE SPECIFIC -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    
    <!-- [if lt IE 9]>
		<%= javascript_include_tag '/hoanmy/js/html5shiv.min.js' %>
		<%= javascript_include_tag '/hoanmy/js/respond.min.js' %>
	<![endif] -->
    
    <!-- BOOTSTRAP STYLE SHEET -->
	<%= stylesheet_link_tag '/hoanmy/css/bootstrap.min.css' %>
    <!-- FONTAWESOME STYLE SHEET -->
	<%= stylesheet_link_tag '/hoanmy/css/font-awesome.min.css' %>
    <!-- FLATICON STYLE SHEET -->
	<%= stylesheet_link_tag '/hoanmy/css/flaticon.min.css' %>
    <!-- ANIMATE STYLE SHEET -->
	<%= stylesheet_link_tag '/hoanmy/css/animate.min.css' %>
    <!-- OWL CAROUSEL STYLE SHEET -->
	<%= stylesheet_link_tag '/hoanmy/css/owl.carousel.min.css' %>
    <!-- BOOTSTRAP SELECT BOX STYLE SHEET -->
	<%= stylesheet_link_tag '/hoanmy/css/bootstrap-select.min.css' %>
    <!-- MAGNIFIC POPUP STYLE SHEET -->
	<%= stylesheet_link_tag '/hoanmy/css/magnific-popup.min.css' %>
    <!-- LOADER STYLE SHEET -->
	<%= stylesheet_link_tag '/hoanmy/css/loader.min.css' %>
    <!-- MAIN STYLE SHEET -->
	<%= stylesheet_link_tag '/hoanmy/css/style.css' %>
    <!-- THEME COLOR CHANGE STYLE SHEET -->
	<%= stylesheet_link_tag '/hoanmy/css/skin.css', class: 'skin' %>
    <!-- CUSTOM  STYLE SHEET -->
	<%= stylesheet_link_tag '/hoanmy/css/custom.css' %>
    <!-- SIDE SWITCHER STYLE SHEET -->
	<%= stylesheet_link_tag '/hoanmy/css/switcher.css' %>

    <!-- REVOLUTION SLIDER CSS -->
	<%= stylesheet_link_tag '/hoanmy/plugins/css/settings.css' %>
    <!-- REVOLUTION NAVIGATION STYLE -->
	<%= stylesheet_link_tag '/hoanmy/plugins/css/navigation.css' %>
    
    <!-- GOOGLE FONTS -->
	<%= stylesheet_link_tag '//fonts.googleapis.com/css?family=Roboto:400,100,300,300italic,400italic,500,500italic,700,700italic,900italic,900' %>
	<%= stylesheet_link_tag '//fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700,800italic,800,700italic' %>
	<%= stylesheet_link_tag '//fonts.googleapis.com/css?family=Crete+Round:400,400i&amp;subset=latin-ext' %>
    
</head>

<body id="bg">
	<div class="page-wraper"> 
        <!-- HEADER START -->
        <%= render "erp/hoanmy/frontend/modules/header" %>
        <!-- HEADER END -->
        
        <!-- CONTENT START -->
        <div class="page-content">
			<%= yield %>            
        </div>
        <!-- CONTENT END -->
        
        <!-- FOOTER START -->
        <%= render "erp/hoanmy/frontend/modules/footer" %>
        <!-- FOOTER END -->
        
        <!-- SCROLL TOP BUTTON -->
        <button class="scroltop">
			<span class=" iconmoon-house relative" id="btn-vibrate"></span>
		</button>
                        
    </div>
 
    <%= render "erp/hoanmy/frontend/modules/javascripts" %>
	<!-- LOADING AREA START ===== -->
	<div class="loading-area">
		<div class="loading-box"></div>
		<div class="loading-pic">
			<div class="loader">
				<span class="block-1"></span>
				<span class="block-2"></span>
				<span class="block-3"></span>
				<span class="block-4"></span>
				<span class="block-5"></span>
				<span class="block-6"></span>
				<span class="block-7"></span>
				<span class="block-8"></span>
				<span class="block-9"></span>
				<span class="block-10"></span>
				<span class="block-11"></span>
				<span class="block-12"></span>
				<span class="block-13"></span>
				<span class="block-14"></span>
				<span class="block-15"></span>
				<span class="block-16"></span>
			</div>
		</div>
	</div>
	<!-- LOADING AREA  END ====== -->
</body>

</html>